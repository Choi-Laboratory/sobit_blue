<?xml version="1.0"?>
<robot name="sobit">

<material name="black">
        <color rgba="0.3 0.3 0.3 0.8"/>
      </material>

<material name="black2">
        <color rgba="0.1 0.1 0.1 1.0"/>
      </material>

  <!--link ####################################################################-->
  <!--base_frame link-->
  <link name="base_frame">
    <visual>
      <geometry>
        <mesh filename="package://sobit_description/mesh/SOBIT_MainFrame v1.stl"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.06"/>
      <material name="black2">
      </material>
    </visual>
  </link>

  <!--left_wheel link-->
  <link name="left_wheel">
    <visual>
      <geometry>
        <mesh filename="package://sobit_description/mesh/wheel.stl"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black">
      </material>
    </visual>
  </link>

  <!--right_wheel link-->
  <link name="right_wheel">
    <visual>
      <geometry>
        <mesh filename="package://sobit_description/mesh/wheel.stl"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black">
      </material>
    </visual>
  </link>

  <!--right_shoulder link-->
  <link name="right_shoulder">
